        <TextBlock Height="23" HorizontalAlignment="Left" Margin="550,12,0,0" Name="textBlock10" Text="SEARCH:" VerticalAlignment="Top" />
        <!--end text blocks-->
        
        <!--text boxes-->
        <TextBox Height="21" HorizontalAlignment="Left" Margin="110,11,0,0" Name="textBox1" VerticalAlignment="Top" Width="150" />
        <TextBox Height="21" HorizontalAlignment="Left" Margin="110,40,0,0" Name="textBox2" VerticalAlignment="Top" Width="150" />
        <TextBox Height="21" HorizontalAlignment="Left" Margin="110,70,0,0" Name="textBox3" VerticalAlignment="Top" Width="150" />
        <TextBox Height="21" HorizontalAlignment="Left" Margin="110,102,0,0" Name="textBox4" VerticalAlignment="Top" Width="150" />
        <TextBox Height="21" HorizontalAlignment="Left" Margin="378,12,0,0" Name="textBox5" VerticalAlignment="Top" Width="150" />
        <TextBox Height="21" HorizontalAlignment="Left" Margin="600,12,0,0" Name="textBox6" VerticalAlignment="Top" Width="150" />
